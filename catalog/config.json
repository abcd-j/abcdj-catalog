{
    "catalog_name": "ABCDJ Catalog",
    "logo_path": "/artwork/abcdj_catalog_logo.svg",
    "link_color": "#151868",
    "link_hover_color": "#3d43d7",
    "social_links": {
        "about": "https://www.abcd-j.de/",
        "documentation": "https://rdm.abcd-j.de/",
        "github": "https://github.com/abcd-j/"
    },
    "dataset_options": {
        "include_datalad": true,
        "include_github": true,
        "include_gitlab": true,
        "include_gin": true,
        "include_binder": true,
        "include_cite": true,
        "include_metadata_export": false,
        "include_access_request": true
    },
    "property_sources": {
        "dataset": {
            "dataset_id": {
                "rule": "single",
                "source": "any"
            },
            "dataset_version": {
                "rule": "single",
                "source": [
                    "catalog_core",
                    "tabby"
                ]
            },
            "type": {
                "rule": "single",
                "source": [
                    "catalog_core",
                    "tabby"
                ]
            },
            "children": {
                "rule": "merge",
                "source": "any"
            },
            "name": {
                "rule": "priority",
                "source": [
                    "tabby"
                ]
            },
            "short_name": {},
            "description": {
                "rule": "priority",
                "source": [
                    "tabby"
                ]
            },
            "doi": {},
            "url": {
                "rule": "merge",
                "source": "any"
            },
            "authors": {
                "rule": "priority",
                "source": [
                    "we_cff",
                    "datacite_gin",
                    "metalad_studyminimeta",
                    "bids_dataset",
                    "tabby",
                    "metalad_core"
                ]
            },
            "keywords": {
                "rule": "merge",
                "source": "any"
            },
            "license": {},
            "funding": {
                "rule": "merge",
                "source": "any"
            },
            "publications": {
                "rule": "merge",
                "source": "any"
            },
            "subdatasets": {
                "rule": "merge",
                "source": "any"
            },
            "additional_display": {
                "rule": "merge",
                "source": "any"
            },
            "top_display": {
                "rule": "merge",
                "source": "any"
            }
        }
    }
}